<div class="container">
  <%= form_with model: @article do |f| %>
  <%= f.label :title, class: "" %>
  <br>
  <%= f.text_field :title, class: "max_width"%>
  <br>
  <%= f.label :content %>
  <br>
  <%= f.text_area :content, class: "max_width mid_height" %>
  <br>
  <%= f.submit "Create article", class: "btn btn-primary" %>
  <% end %>
  <%= link_to "back", articles_path %>
</div>
